<%= render "layouts/components/banner" %>
<div class = "container">
  <div class="cards">
    <% @lists.each do |list|  %>
      <%= link_to list_path(list) do %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3),
      rgba(0,0,0,0.3)), url('<%= cl_image_path(list.photo.key)%>')">
      <%= list.name %>
      </div>
      <% end %>
    <% end %>
  </div>
</div>
